<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Title>Cloudflare Workers KV Client</Title>
    <Description>Cloudflare Workers KV Client for .NET</Description>
    <PackageTags>cloudflare;workers;kv;client</PackageTags>
    <Version>1.1.0</Version>
    <TargetFrameworks>netstandard1.3;netstandard1.4;netstandard1.6;netstandard2.0</TargetFrameworks>
    <PackageLicenseUrl>https://raw.githubusercontent.com/aozd4v/cloudflare-workers-kv-dotnet-client/master/LICENSE</PackageLicenseUrl>
    <PackageId>CloudflareWorkersKv.Client</PackageId>
    <AssemblyName>CloudflareWorkersKv.Client</AssemblyName>
    <RepositoryUrl>https://github.com/aozd4v/cloudflare-workers-kv-dotnet-client</RepositoryUrl>
    <Authors>aozd4v</Authors>
    <PackageProjectUrl>https://github.com/aozd4v/cloudflare-workers-kv-dotnet-client</PackageProjectUrl>
    <PackageIconUrl>https://raw.githubusercontent.com/aozd4v/cloudflare-workers-kv-dotnet-client/master/logo/logo.png</PackageIconUrl>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0" PrivateAssets="All" />
  </ItemGroup>


  <ItemGroup>
    <PackageReference Include="Flurl.Http" Version="2.4.1" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.2" />
  </ItemGroup>

</Project>
